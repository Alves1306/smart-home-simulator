    {
        "id": "ecd99a872ed46d69",
        "type": "function",
        "z": "8cf5e70e77b84c28",
        "name": "function 5",
        "func": "let modo = flow.get(\"modo\") || \"auto\";\nif (modo !== \"auto\") return null;\n\nlet d = msg.payload;\nlet now = new Date().getTime();\n\n// === PRESENÇA (com base em movimento recente) ===\nif (d.motion === true) {\n    flow.set(\"presenca\", true);\n    flow.set(\"presenca_timer\", now);\n}\n\nlet tempoUltimoMov = flow.get(\"presenca_timer\") || 0;\nlet tempoDesdeMov = now - tempoUltimoMov;\nlet presenca = tempoDesdeMov <= 30000; // considera presença durante 30s após movimento\n\n// === LÓGICA DE CONTROLO ===\nlet luzCmd = \"OFF\";\nlet fanCmd = \"OFF\";\nlet janelaCmd = \"CLOSE\";\n\n// === LUZ ===\nif (presenca && d.lux < 300) {\n  luzCmd = \"ON\";\n} else {\n  luzCmd = \"OFF\";\n}\n\n// === FAN E JANELA ===\nif (presenca && d.temp_in > 28) {\n  if (d.temp_out >= 18 && d.temp_out <= 24) {\n    janelaCmd = \"OPEN\";\n    fanCmd = \"OFF\";\n  } else {\n    janelaCmd = \"CLOSE\";\n    fanCmd = \"ON\";\n  }\n}\n\nreturn [\n  { payload: luzCmd, topic: \"home/light/set\" },\n  { payload: fanCmd, topic: \"home/fan/set\" },\n  { payload: janelaCmd, topic: \"home/window/set\" }\n];\n",
