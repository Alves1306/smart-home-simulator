        "func": "let raw = msg.payload;\nlet data;\n\ntry {\n  data = (typeof raw === 'string') ? JSON.parse(raw) : raw;\n} catch (err) {\n  node.warn(\"Failed to parse JSON: \" + err.message);\n  return null;\n}\n\nif (typeof data !== 'object') {\n  node.warn(\"Payload is not an object\");\n  return null;\n}\n\nreturn [\n  { payload: data.temp_in ?? \"?\" },\n  { payload: data.humidity ?? \"?\" },\n  { payload: data.lux ?? \"?\" },\n  { payload: data.motion === true ? \"Yes\" : data.motion === false ? \"No\" : \"?\" },\n  { payload: data.temp_out ?? \"?\" }\n];\n",
